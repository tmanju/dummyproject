<?xml version="1.0" encoding="UTF-8"?>
<canvas id="promotionPolicy" type="search"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.thirdpillar.com/codify/ui-metamodel ui-metamodel.xsd"
	xmlns="http://www.thirdpillar.com/codify/ui-metamodel">

	<panel id="promotionPolicy.promotionPolicySearch" label="Promotion Search">
		<field name="promotionPolicySearch.name" label="Promotion Name" />
		<field name="promotionPolicySearch.refNumber" label="Promotion Number" />
		<field name="promotionPolicySearch.promotionType" label="Type" />
		<field name="promotionPolicySearch.effectiveFromDate" label="Effective Date" />
		<field name="promotionPolicySearch.effectiveToDate" label="Through" />
		<field name="promotionPolicySearch.ou" label="OU" />
		<field name="promotionPolicySearch.sbu" label="SBU" />
