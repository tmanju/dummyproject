<?xml version="1.0" encoding="UTF-8"?>
<canvas id="team" type="list"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.thirdpillar.com/codify/ui-metamodel ui-metamodel.xsd"
		xmlns="http://www.thirdpillar.com/codify/ui-metamodel">
	
	<data-table name="results" row="team">
	<columns>
			<column name="team.name"/>
			<column name="team.description"/>
			<column name="team.email"/>
			<column name="team.teamCategory"/>
			<column name="team.status"/>
			
		</columns>
	<commands>
		<command name="Export CSV" action="Team-ExportCSV" type="exportCsv"/>
	</commands>	
		
	</data-table>
	<beans>
		<bean name="results" type="com.thirdpillar.codify.loanpath.model.Team" />
	</beans>	
</canvas>
