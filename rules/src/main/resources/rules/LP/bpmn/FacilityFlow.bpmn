    <scriptTask id="_55" name="Check No. Of Facilities in Application" scriptFormat="http://www.java.com/java" >
      <script>Facility fac = (Facility) context.getVariable("contextEntity");
String match ="";
Request request = fac.getRequest();
match ="false";
